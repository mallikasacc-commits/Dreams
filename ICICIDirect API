<!-- Added-->
<table class="tableizer-table">
<thead><tr class="tableizer-firstrow"><th>Parameter </th><th>Type </th><th>Description/Remark</th></tr></thead><tbody>
 <tr><td>ErrMsg</td><td>string </td><td>Returns system Error</td></tr>
</tbody></table>
<pre id="Pre66" class="spec-example"><b>Request Example:-</b>
<br>
{"DeviceId":"AAAABBBBCCCCDDD","MobileNumber":"9638527410"}
</pre>  
<pre id="Pre67" class="spec-example"><b>Response Example:-</b>
<br>{
	"Status": 200,
	"Error": null,
	"Success": {
		"UserId": ""
	}
}
</pre>
