 <tr><td>&nbsp;</td><td>EXCHANGE</td><td>String</td><td>Yes</td><td>Yes</td><td>"NFO"</td></tr>
 <tr><td>&nbsp;</td><td>OPERATION_TYP</td><td>String</td><td>N/A</td><td>Yes</td><td>"F" when underlying is blank else "D" when underlying is given. </td></tr>
 <tr><td>&nbsp;</td><td>PRDCT_TYP</td><td>String</td><td>N/A</td><td>Yes</td><td>Futures/Options</td></tr>
 <tr><td>&nbsp;</td><td>UNDRLYNG</td><td>String</td><td>N/A</td><td>Yes</td><td>Stock code (optional)</td></tr>
 <tr><td>&nbsp;</td><td>ORDR_STTS</td><td>String</td><td>Yes</td><td>Yes</td><td>"A" (ALL)</td></tr> 
 <tr><td>Checksum</td><td>&nbsp;</td><td>String</td><td>N/A</td><td>Yes</td><td>Combination of ( Time Stamp + JSONPostData + secret_key )</td></tr>
</tbody></table>
