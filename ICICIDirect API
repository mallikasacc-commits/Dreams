            "one_clk_tran_id": "0",
            "one_clk_plan_id": "0",
            "plan_name": "",
            "plan_text": "",
            "one_clk_bskt_id": "0",
            "basket_name": ""
        }
    ]
}
</pre>

</section>
 <section id="sec-mfotp" secid="4.8">
<h1>Generate OTP</h1>

<!--<h4>API URL:<i>https://api.icicidirect.com/content/genotp</i> </h4>
<h2>Generate OTP</h2>
<br/> <b>Input Parameters:-</b>-->

<!-- Added-->
 <table class="tableizer-table">
<thead>
<tr class="tableizer-firstrow">
<th style="text-align:left">Request</th>
<th colspan="5" style="text-align:left">https://api.icicidirect.com/content/genotp</th></tr>
</thead>
</table>
<br>
<h2>Generate OTP</h2>

<!-- Added-->
<table class="tableizer-table">
<thead><tr class="tableizer-firstrow"><th>Parameter </th><th>Type</th><th>Required</th><th>Description/Remark</th></tr></thead><tbody>
 <tr><td>DeviceId </td><td>String</td><td>Yes</td><td>Device Id </td></tr>
 <tr><td>MobileNumber </td><td>String</td><td>Yes</td><td>Mobile Number</td></tr>
</tbody></table>
<br>

<!--<b>Output Parameters:-</b><br/>-->

<b><i>NOTE: OTP will be send on register Email Id</i></b>
<!--Added-->
 <table class="tableizer-table">
