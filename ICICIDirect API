 <tr><td>ical_ANSWR_FLAG         </td><td>string </td></tr>
 <tr><td>ical_ACTIVE             </td><td>string </td></tr>
 <tr><td>ical_VALID_FLAG         </td><td>string </td></tr>
 <tr><td>ical_MRGN_FLAG          </td><td>string </td></tr>
 <tr><td>ical_LOW_RT             </td><td>string </td></tr>
 <tr><td>ical_QUOTE              </td><td>string </td></tr>
 <tr><td>ical_TRNSCTN_PRC        </td><td>string </td></tr>
 <tr><td>ical_NN_HIGH_PRICE      </td><td>string </td></tr>
 <tr><td>ical_NN_LOW_PRICE       </td><td>string </td></tr>
 <tr><td>ical_ORD_STP_LSS        </td><td>string </td></tr>
 <tr><td>ical_STCK_CD            </td><td>string </td></tr>
 <tr><td>ical_XCHNG_CD           </td><td>string </td></tr>
 <tr><td>ical_USR_ADDRSS_LN1     </td><td>string </td></tr>
 <tr><td>ical_USR_ADDRSS_LN2     </td><td>string </td></tr>
 <tr><td>ical_ARD_CRNNT_INVSTR   </td><td>string </td></tr>
 <tr><td>ical_ARD_PRDCT_TYP_RQSTD</td><td>string </td></tr>
 <tr><td>ical_STK_STCK_NM        </td><td>string </td></tr>
 <tr><td>ical_TRD_DT             </td><td>string </td></tr>
 <tr><td>ical_CHART_TYPE         </td><td>string </td></tr>
 <tr><td>ical_ARD_CLNT_RMRKS     </td><td>string </td></tr>
 <tr><td>ical_EXPIRY_DATE        </td><td>string </td></tr>
 <tr><td>ErrMsg</td><td>String</td></tr>
</tbody></table>

